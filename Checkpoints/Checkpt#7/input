main; 
    data: 
        integer: x, y[10];  # array references with variabels in [] expression 
        real: z, fido;
    algorithm: 
        x := 10; 
        z := x + 3.14;  # arithmic expr 
        fido := x & z; 
        fido := x | z;  # logic op
        y[1] := 5 < 6;  # comparison & array subscript
        y[2] := 1 <> 0; 
        y[3] := x * 3; 
        y[4] := 10/5; 
        y[5] := (3 + 5) * 7; 
        y[6] := y[1] - y[2]; 
        y[y[3] / 3] := 0; 
end main; 